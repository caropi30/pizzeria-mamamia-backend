# Configuracion del editor, mira https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trainling_whitespace = true

[*.html]
indent_size = 4

[*{.ts, .js}]
quote_type = single
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false
